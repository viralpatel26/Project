<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ContactUs</servlet-name>
        <servlet-class>servlets.ContactUs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>servlets.home</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ContactUs</servlet-name>
        <url-pattern>/contactUs</url-pattern>
    </servlet-mapping>
    <env-entry>
        <env-entry-name>webmail-username</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>viralpatel2697@gmail.com</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>webmail-password</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>SNAPchat@2020</env-entry-value>
    </env-entry>
    <servlet>
        <servlet-name>subscribe</servlet-name>
        <servlet-class>servlets.subscribe</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>subscribe</servlet-name>
        <url-pattern>/subscription</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>contactUs</welcome-file>
    </welcome-file-list>
</web-app>
